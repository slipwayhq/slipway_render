{
    "type": "AdaptiveCard",
    "$schema": "../../../../adaptive_cards_data/schema/adaptive-card.schema.json",
    "version": "1.6",
    "body": [
        {
            "type": "Table",
            "showGridLines": true,
            "gridStyle": "accent",
            "columns": [
                {
                    "width": 1
                },
                {
                    "width": 2
                },
                {
                    "width": "50px"
                }
            ],
            "rows": [
                {
                    "type": "TableRow",
                    "cells": [
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Narrow",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "style": "accent",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Wide",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "A",
                                    "wrap": true
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "TableRow",
                    "cells": [
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Narrow 2",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "style": "accent",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Wide 2",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "style": "accent",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Fixed 2",
                                    "wrap": true
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "TableRow",
                    "style": "accent",
                    "cells": [
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Narrow 3",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Wide 3",
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "TableCell",
                            "items": [
                                {
                                    "type": "TextBlock",
                                    "text": "Fixed 3",
                                    "wrap": true
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}